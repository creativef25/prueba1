{% extends 'base.html.twig' %}

{% block body %}
    <h1 class="center">Usuario Nuevo</h1>

    {{ form_start(form) }}
    <div class="container">
      <div class="row">
        <div class="col s10">
          <div>
          {{ form_label(form.nombre)}}
          {{ form_widget(form.nombre) }}
          {{ form_errors(form.nombre)}}
          </div>
        </div>
      </div>
    </div>
        <input type="submit" class="btn waves-effect waves-light black" value="Crear" />
    {{ form_end(form) }}

    <ul>
        <li>
            <a href="{{ path('usuario_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
